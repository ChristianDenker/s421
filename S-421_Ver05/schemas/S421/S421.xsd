	<!--                                      -->
	<!-- "BooleanType" element type definition -->
	<!--                                      -->
	<xs:simpleType name="Boolean">
		<xs:annotation>
			<xs:documentation>RealBoolean Type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<!--                                      -->
	<!-- "TextType" element type definition -->
	<!--                                      -->
	<!-- For Example, routeFormatVersion -->
	<xs:simpleType name="Text">
		<xs:annotation>
			<xs:documentation>String Type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="NonEmptyText">
		<xs:annotation>
			<xs:documentation>String Type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([a-zA-Z|\d]+[a-zA-Z|\d|W.]+[a-zA-Z|\d])"/>
			<!--		<xs:pattern value="([a-zA-Z|\d]+[a-zA-Z|\d|W.|\s|W_|W-]+[a-zA-Z|\d])"/>
-->
		</xs:restriction>
	</xs:simpleType>
	<!--                                      -->
	<!-- "IMONumberText : 7 digit" element type definition -->
	<!--                                      -->
	<xs:simpleType name="IMONumberText">
		<xs:annotation>
			<xs:documentation>String Type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="7"/>
			<xs:pattern value="\d{7}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                                      -->
	<!-- "MMSINumberText : 9 digit" element type definition -->
	<!--                                      -->
	<xs:simpleType name="MMSINumberText">
		<xs:annotation>
			<xs:documentation>String Type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="9"/>
			<xs:pattern value="\d{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                                      -->
	<!-- "IntegerType" element type definition -->
	<!--                                      -->
	<xs:simpleType name="Integer">
		<xs:annotation>
			<xs:documentation>Integer Type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer"/>
	</xs:simpleType>
	<!--                                      -->
	<!-- "PositiveIntegerType" element type definition -->
	<!--                                      -->
	<xs:simpleType name="PositiveInteger">
		<xs:annotation>
			<xs:documentation>Positive Integer Type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                                      -->
	<!-- "RealType" element type definition -->
	<!--                                      -->
	<xs:simpleType name="Real">
		<xs:annotation>
			<xs:documentation>RealType.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<!--                                      -->
	<!-- "NonNegativeReal" element type definition -->
	<!--                                      -->
	<xs:simpleType name="NonNegativeReal">
		<xs:annotation>
			<xs:documentation>NonNegativeReal type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0.0"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                                      -->
	<!-- "NonNegativeRealWith1DecimalPlace" element type definition -->
	<!--                                      -->
	<xs:simpleType name="NonNegativeRealWith1DecimalPlace">
		<xs:annotation>
			<xs:documentation>NonNegativeReal type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0.0"/>
			<xs:fractionDigits value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                                      -->
	<!-- "NonNegativeRealWith1DecimalPlace" element type definition -->
	<!--                                      -->
	<xs:simpleType name="NonNegativeRealWith2DecimalPlace">
		<xs:annotation>
			<xs:documentation>NonNegativeReal type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0.00"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                                      -->
	<!-- "RadiusType" element type definition -->
	<!--                                      -->
	<xs:simpleType name="RadiusType">
		<xs:annotation>
			<xs:documentation>Radius type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0.0"/>
			<xs:maxExclusive value="5.01"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                     -->
	<!-- XTD type definition -->
	<!--                     -->
	<xs:simpleType name="XtdType">
		<xs:annotation>
			<xs:documentation>
			XTD of the point. Nautical miles.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="1.0"/>
			<xs:maxExclusive value="10000.0"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                        -->
	<!-- DateTime definition -->
	<!--                        -->
	<xs:simpleType name="DateTime">
		<xs:annotation>
			<xs:documentation>
			The DateTime is a combination of a date and a time; for example, [-]CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm].
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<!--                                      -->
	<!-- "PortIDType : 5 character digit"  definition -->
	<!--                                      -->
	<xs:simpleType name="PortIDType">
		<xs:annotation>
			<xs:documentation>String Type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="5"/>
			<xs:pattern value="[a-z]{5}"/>
			<!--			<xs:pattern value="[a-zA-Z]{5}"/>
-->
		</xs:restriction>
	</xs:simpleType>
	<!-- ============================================================ -->
	<!-- Simple type End for S421  -->
	<!-- ============================================================ -->
	<!-- ============================================================ -->
	<!-- ============================================================ -->
	<!-- Complex types Start for S421  -->
	<!-- ============================================================ -->
	<!-- ============================================================ -->
	<!--                                      -->
	<!-- "routeExtensions" element type definition  -->
	<!--                                      -->
	<xs:complexType name="RouteExtensions">
		<xs:annotation>
			<xs:documentation>
			Route Extensions
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="InformationTypeType">
				<xs:sequence>
					<xs:element name="routeExtensionsNote" type="Text" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Route Extension Note</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="routeExtensionsManufacturerId" type="Text" use="required">
					<xs:annotation>
						<xs:documentation>
						Unique vendor identifier for the Route extensions
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="routeExtensionsName" type="Text" use="optional">
					<xs:annotation>
						<xs:documentation>
						Route extension name
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="routeExtensionsVersion" type="Text" use="optional">
					<xs:annotation>
						<xs:documentation>
						RouteExtension version 
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--                              -->
	<!-- Root element type definition -->
	<!--                              -->
	<xs:element name="route" type="Route" substitutionGroup="FeatureType"/>
	<xs:complexType name="Route">
		<xs:annotation>
			<xs:documentation>
        RTZ files contain a number of waypoints, followed with auxiliary schedules. You can add your own elements to the extension 	section of the RTZ document.
		</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FeatureType">
				<xs:sequence>
					<xs:element name="routeFormatVersion" type="Text" fixed="1.0" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							The version of this route format
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeID" type="NonEmptyText" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Unique Route Identifier, At least three characters of alphanumeric and periods. The first and last letters are alphanumeric only
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeEditionNo" type="PositiveInteger" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Route Edition number
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfo" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="routeWaypoints" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeSchedules" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeActionPoints" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
							Additional proprietary information for the route specified by the manufacturer.
						</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--                                      -->
	<!-- "RouteInfo" element type definition  -->
	<!--                                      -->
	<xs:element name="routeInfo" type="RouteInfo" substitutionGroup="InformationType"/>
	<xs:complexType name="RouteInfo">
		<xs:annotation>
			<xs:documentation>
			list of route information
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="InformationTypeType">
				<xs:sequence>
					<xs:element name="routeInfoName" type="NonEmptyText" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Name of the route, At least three characters of alphanumeric and periods. The first and last letters are alphanumeric only</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoAuthor" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Author of the route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoEditionTime" type="DateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Time for this edition of route plan is edited </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoDescription" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Additional information for the status of the route 
							i.e. specific error message and/or cause of the route change and/or any other detail to consider before 																							
							futher use of this route plan and/or the purpose of sending a route plan and/or why this route is
							recommended.																															
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoStatus" type="RouteInfoStatus" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Enumeration of Status of route. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoValidityStart" type="DateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Start of validity of route plan period </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoValidityEnd" type="DateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Stop of validity of route plan period</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoDeparturePortID1" type="PortIDType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Primary identifier for departure port given by UN/LOCODE</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoDeparturePortID2" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Secondary identifier for departure port given by free text string</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoDeparturePortCall" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reference to port call identifier in departinng port</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoArrivalPortID1" type="PortIDType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Primary identifier for arrival port given by UN/LOCODE</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoArrivalPortID2" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Secondary identifier for arrival port given by free text string</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoArrivalPortCall" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reference to port call identifier in arriving port</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoReferencePrevRoute" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reference previous route. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoReferenceNextRoute" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Reference Next route. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselType" type="RouteInfoVesselType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Type of vessel</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselName" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Ship’s name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselMMSI" type="MMSINumberText" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Ship’s MMSI (XXXXXXXXX)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselCallsign" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Ship’s Call sign </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselIMO" type="IMONumberText" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Ship’s IMO number (XXXXXXX)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselVoyage" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Unique voyage identifier</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselHeight" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Vessel height measured from the keel to the masthead</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselLength" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Vessel length overall (LOA)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoVesselBeam" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Vessel beam</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoDraftMax" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum draft of the route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoAirDraftMax" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum Air draft of the route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoBeamMax" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum Vessel beam of the route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoLengthMax" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum Vessel length of the route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeInfoExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
							Additional proprietary information for the route info specified by the manufacturer
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- "RouteWaypoints" element type definition  -->
	<!--                                      -->
	<xs:element name="routeWaypoints" type="RouteWaypoints" substitutionGroup="FeatureType"/>
	<xs:complexType name="RouteWaypoints">
		<xs:complexContent>
			<xs:extension base="FeatureType">
				<xs:sequence>
					<xs:element name="routeWaypointsCollection" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="routeWaypoint" type="gml:ReferenceType" minOccurs="2" maxOccurs="unbounded"/>
					<xs:element name="routeWaypointsExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
							Additional proprietary information for the waypoints specified by the manufacturer
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--                                           -->
	<!-- "RouteWaypointDefault" element type definition -->
	<!--                                           -->
	<!--                                    -->
	<!-- "RouteWaypoint" element type definition -->
	<!--                                    -->
	<xs:element name="routeWaypoint" type="RouteWaypoint" substitutionGroup="FeatureType"/>
	<xs:complexType name="RouteWaypoint">
		<xs:complexContent>
			<xs:extension base="FeatureType">
				<xs:sequence>
					<xs:element name="geometry" type="GM_Point" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Geographical position of the waypoint in degrees</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointID" type="PositiveInteger" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Identifier of the waypoints. The identifier shall be  unique in the system</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointName" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Name of waypoint</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointExternalReferenceID" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>External reference information related with the waypoint</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointFixed" type="Boolean" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Indicator if the route waypoint is fixed(true) or editable(false)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointTurnRadius" type="RadiusType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Turn radius in nautical miles</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointCollection" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="routeWaypointLeg" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeWaypointExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
							Additional proprietary information for the Route waypoints specified by the manufacturer
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--                               -->
	<!-- "RouteWaypointLeg" element type definition -->
	<!--                               -->
	<xs:element name="routeWaypointLeg" type="RouteWaypointLeg" substitutionGroup="FeatureType"/>
	<xs:complexType name="RouteWaypointLeg">
		<xs:complexContent>
			<xs:extension base="FeatureType">
				<xs:sequence>
					<xs:element name="routeWaypointLegStarboardXTDL" type="XtdType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Starboard cross track distance limit </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegPortXTDL" type="XtdType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Port cross track distance limit</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegStarboardCL" type="XtdType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Starboard cross track distance check limit</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegPortCL" type="XtdType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Port cross track distance check limit</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegSafetyContour" type="NonNegativeRealWith1DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Planned safety contour </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegSafetyDepth" type="NonNegativeRealWith1DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Planned safety depth</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegGeometryType" type="RouteWaypointLegGeometryType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Geometry type of leg; 1: Loxodrome, 2: Orthodrome </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegSOGMin" type="NonNegativeReal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Regulatory Lowest speed over ground</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegSOGMax" type="NonNegativeReal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Regulatory Highest speed over ground</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegSTWMin" type="NonNegativeReal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Minimum safe speed through water</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegSTWMax" type="NonNegativeReal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum safe speed through water</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegDraft" type="NonNegativeRealWith1DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Planned static Draft, maximum </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegDraftForward" type="NonNegativeRealWith1DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Planned static Draft Forward </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegDraftAft" type="NonNegativeRealWith1DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Planned static Draft Aft </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegDraftMax" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum draft of the route waypoint leg for all vessles </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegAirDraftMax" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum air draft of the route waypoint leg for all vessels </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegBeamMax" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum vessel beam  of the route waypoint leg for all vessels</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegLengthMax" type="NonNegativeRealWith2DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Maximum vessel length  of the route waypoint leg for all vessels</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegStaticUKC" type="NonNegativeRealWith1DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Minimum static UKC on the leg</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegDynamicUKC" type="NonNegativeRealWith1DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Minimum dynamic UKC on the leg</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegSafetyMargin" type="NonNegativeRealWith1DecimalPlace" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Draft component including uncertainties </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegNote" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Note for the route waypoint leg</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegIssue" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Safety Issue for the route waypoint leg</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeWaypointLegCollection" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="routeWaypointLegExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
							Additional proprietary information for the Route waypoint Legs specified by the manufacturer
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--                                              -->
	<!-- "RouteActionPoints" element type definition -->
	<!--                                              -->
	<xs:element name="routeActionPoints" type="RouteActionPoints" substitutionGroup="FeatureType"/>
	<xs:complexType name="RouteActionPoints">
		<xs:complexContent>
			<xs:extension base="FeatureType">
				<xs:annotation>
					<xs:documentation>
					RouteActionPoints definition.
					</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="routeActionPointsCollection" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="routeActionPoint" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="routeActionPointsExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
							Additional proprietary information for the action points specified by the manufacturer
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--                                           -->
	<!-- "RouteActionPoint" element type definition -->
	<!--                                           -->
	<xs:element name="routeActionPoint" type="RouteActionPoint" substitutionGroup="FeatureType"/>
	<xs:complexType name="RouteActionPoint">
		<xs:complexContent>
			<xs:extension base="FeatureType">
				<xs:sequence>
					<xs:element name="geometry" type="RouteActionPointSpaceGeometry" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Geographical position of the action point in degrees
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeActionPointID" type="PositiveInteger" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Identifier of the action point. The identifier shall be  unique in the route
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeActionPointName" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Name of action point
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeActionPointExternalReference" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							External reference information related with the action point
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeActionPointRadius" type="Real" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Distance from the action point within which action must be taken. 
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeActionPointTimeToAct" type="Integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Time before or after action point
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeActionPointRequiredAction" type="RouteActionPointRequiredAction" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							The action required for this action
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeActionPointRequiredActionDescription" type="Text" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Human readable description of the action required for this action point
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeActionPointCollection" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="routeActionPointExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
							Additional proprietary information for the action points specified by the manufacturer
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
