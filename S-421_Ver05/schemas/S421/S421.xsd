							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleCollection" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="routeScheduleManual" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeScheduleCalculated" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeScheduleRecommended" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeScheduleExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
