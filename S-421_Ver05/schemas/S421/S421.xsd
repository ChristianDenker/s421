	<!-- "RouteScheduleElement" element type definition -->
	<!--                                           -->
	<xs:element name="routeScheduleElement" type="RouteScheduleElement" substitutionGroup="InformationType"/>
	<xs:complexType name="RouteScheduleElement">
		<xs:complexContent>
			<xs:extension base="InformationTypeType">
				<xs:sequence>
					<xs:element name="routeWaypointId" type="PositiveInteger" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Waypoint identifier for the schedule element
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementPlanSOG" type="NonNegativeReal" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Planned speed over ground
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementETD" type="DateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Planned  or Recommended Time of departure represented by Estimated Time of departure (ETD)  
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementETA" type="DateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Planned or Recommended Time of arrival represented by Estimated time of arrival (ETA)  
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementETDWindowBefore" type="Integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Describes the uncertainty of the predicted ETD before planned time or recommended time 
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementETDWindowAfter" type="Integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Describes the uncertainty of the predicted ETD after planned time or recommended time
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementETAWindowBefore" type="Integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Describes the uncertainty of the predicted ETA before planned time or recommended time
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementETAWindowAfter" type="Integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Describes the uncertainty of the predicted ETA after planned time or recommended time
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementNote" type="Text" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
							Notes for the route schedule element
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="routeScheduleElementManualCollection" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeScheduleElementCalculatedCollection" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeScheduleElementRecommendedCollection" type="gml:ReferenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="routeScheduleElementExtensions" type="RouteExtensions" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
							Additional proprietary information for the route schedules specified by the manufacturer
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
